<script type="text/javascript"><!--display bar only if file is chosen-->

$(document).ready(function() { 
//

//show the progress bar only if a file field was clicked
	var show_bar = 0;
    $('input[type="file"]').click(function(){
		show_bar = 1;
    });

//show iframe on form submit
    $("#form").submit(function(){

		if (show_bar === 1) { 
			$('#upload_frame').show();
			function set () {
				$('#upload_frame').attr('src','/upload.php?up_id=4f82d2ded2199');
			}
			setTimeout(set);
		}
    });
//

});
</script>
